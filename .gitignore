backend/.DS_Store

# Ignore local .env files
backend/.env.development.local
backend/.env.test.local
backend/.env.production.local

# Ignore the .eslintcache directory
backend/.eslintcache/

# Ignore the .now directory for Zeit Now deployments
backend/.now/


